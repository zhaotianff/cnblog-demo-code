<Window x:Class="CommunityToolkitMVVMIocDemo_MVVM_legency.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommunityToolkitMVVMIocDemo_MVVM_legency"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label Content="数1"></Label>
        <TextBox Width="300" HorizontalAlignment="Left" Text="{Binding Num1}"></TextBox>
        <Label Content="数2"></Label>
        <TextBox Width="300" HorizontalAlignment="Left" Text="{Binding Num2}"></TextBox>
        <Button Content="加" Command="{Binding AddCommand}"></Button>
        <Button Content="减" Command="{Binding SubCommand}"></Button>
    </StackPanel>
</Window>
